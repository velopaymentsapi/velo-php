{
    "name": "velopaymentsapi/velo-php",
    "version": "2.37.151-RC1",
    "description": "This library provides a PHP client that simplifies interactions with the Velo Payments API.",
    "keywords": [
        "velo",
        "velo-payments",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/velopaymentsapi/velo-php",
    "license": "unlicense",
    "authors": [
        {
            "name": "VeloPayments contributors",
            "homepage": "https://github.com/velopaymentsapi/velo-php"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "VeloPayments\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "VeloPayments\\Client\\Test\\" : "test/" }
    }
}
