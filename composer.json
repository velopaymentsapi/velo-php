{
    "name": "velopaymentsapi/velo-php",
    "version": "2.16.18",
    "description": "This library provides a PHP client that simplifies interactions with the Velo Payments API.",
    "keywords": [
        "velo",
        "velo-payments",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/velopaymentsapi/velo-php",
    "license": "unlicense",
    "authors": [
        {
            "name": "VeloPayments contributors",
            "homepage": "https://github.com/velopaymentsapi/velo-php"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "VeloPayments\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "VeloPayments\\Client\\" : "test/" }
    }
}
